page {
  background-color: #ffffff;
  font-size: 26rpx;
}

.shoop {
  display: flex;
  justify-content: space-between;
  background-color: #F8F8F8;
  padding: 20rpx;
}

.commodity {
  padding: 0rpx 20rpx;
  margin-bottom: 10rpx;
  background-color: #fff;
}

.commodity view:nth-child(1) {
  border-top: none;
}

.commodity .commodity_list {
  display: flex;
  flex-direction: column;
  border-top: 1rpx solid #ccc;
  padding: 30rpx 0rpx;
}

.commodity .commodity_list .commodity_list_head {
  margin-bottom: 30rpx;
}

.commodity .commodity_list .commodity_list_bottom {
  display: flex;
  justify-content: space-between;
}

.commodity .commodity_list .commodity_list_bottom .jisuan {
  display: flex;
}

.commodity .commodity_list .commodity_list_bottom .jisuan view {
  width: 40rpx;
  height: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.commodity .commodity_list .commodity_list_bottom .jisuan .item {
  padding: 0rpx 30rpx;
}

.commodity .commodity_list .commodity_list_bottom .jisuan .edd {
  border: 1rpx solid #ccc;
  border-radius: 50%;
  text-align: center;
}

.commodity .commodity_list .commodity_list_bottom .clear {
  width: 40rpx;
  height: 40rpx;
  color: #fff;
  background-color: red;
  text-align: center;
  line-height: 33rpx;
  border-radius: 50%;
}

.shopBag {
  padding: 30rpx 20rpx;
  background-color: #fff;
  margin-bottom: 10rpx;
}

.shopBag .bag {
  margin-bottom: 40rpx;
  display: flex;
  justify-content: space-between;
}

.shopBag .buyBag {
  position: relative;
  display: flex;
  justify-content: space-between;
}

.shopBag .buyBag .Bag_right {
  margin-right: 120rpx;
}

.shopBag .buyBag .Bag_right::before {
  content: '';
  height: 77px;
  border: 1px dashed #D2D2D2;
  position: absolute;
  left: 352rpx;
  top: 0;
  bottom: 0;
  margin: auto;
}

.shopBag .buyBag .Bag_x {
  flex: 1;
  display: flex;
}

.shopBag .buyBag .Bag_x .Bag_x_img {
  flex: 1;
  display: flex;
  justify-content: center;
  margin-right: 20rpx;
}

.shopBag .buyBag .Bag_x .Bag_x_img image {
  width: 70%;
}

.shopBag .buyBag .Bag_x .Bag_x_img .Bag_x_imgd {
  width: 80%;
}

.shopBag .buyBag .Bag_x .Bag_x_item {
  flex: 1.5;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  color: #999999;
}

.shopBag .buyBag .Bag_x .Bag_x_item .jisuan {
  display: flex;
}

.shopBag .buyBag .Bag_x .Bag_x_item .jisuan view {
  width: 40rpx;
  height: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  line-height: 38rpx;
}

.shopBag .buyBag .Bag_x .Bag_x_item .jisuan .item {
  padding: 0rpx 30rpx;
  color: #000;
}

.shopBag .buyBag .Bag_x .Bag_x_item .jisuan .edd {
  border: 1rpx solid #ccc;
  border-radius: 50%;
}

.invoice {
  position: relative;
  padding: 20rpx;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
  margin-bottom: 10rpx;
}

.invoice .invoice_icon {
  padding-right: 30rpx;
  color: #999999;
}

.invoice .invoice_icon::after {
  content: "\e633";
  font-family: "iconfont";
  font-size: 24rpx;
  position: absolute;
  top: 30rpx;
  right: 14rpx;
}

.prices {
  padding: 20rpx;
  height: 100%;
  background-color: #fff;
}

.prices .prices_num {
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
}

.prices .prices_disc {
  display: flex;
  justify-content: space-between;
}

.prices .prices_disc .discColor {
  color: var(--bulueColor);
}

.close {
  position: fixed;
  bottom: 0;
  display: flex;
  border-top: 1rpx solid #ccc;
  width: 100%;
  height: 120rpx;
  background-color: #fff;
}

.close view {
  display: flex;
  align-items: center;
  justify-content: center;
}

.close .close_sm {
  flex: 2;
  position: relative;
}

.close .close_sm .close_tj {
  position: absolute;
  top: -70rpx;
  left: 45rpx;
  width: 140rpx;
  height: 70rpx;
  border-top: 1rpx solid #ccc;
  border-left: 1rpx solid #ccc;
  border-right: 1rpx solid #ccc;
  border-radius: 100px 100px 0rpx 0rpx;
  background-color: #fff;
}

.close .close_sm .close_icon {
  position: absolute;
  top: -60rpx;
  left: 55rpx;
  width: 120rpx;
  height: 120rpx;
  background-color: #F47B20;
  border-radius: 50%;
}

.close .close_sm .close_icon::after {
  content: "\e6b9";
  font-family: "iconfont";
  font-size: 80rpx;
  position: absolute;
  top: 18rpx;
  right: 20rpx;
  color: #fff;
}

.close .close_sm .close_txm {
  position: absolute;
  top: -65rpx;
  left: 246rpx;
  padding: 10rpx 30rpx;
  font-size: 18rpx;
  width: 280rpx;
  border-radius: 20rpx;
  color: #999999;
  border: 1rpx solid #999999;
}

.close .close_sm .close_txm text {
  color: var(--bulueColor);
}

.close .close_hj {
  flex: 4;
}

.close .close_hj text {
  color: #F47B20;
  margin-left: 10rpx;
}

.close .close_js {
  flex: 3;
  background-color: #F47B20;
  color: #fff;
}

.close .close_js2 {
  background-color: #D2D2D2;
}

.close .clearCart {
  flex: 3;
  background-color: red;
  color: #fff;
}

.orgColor {
  color: #F47B20;
}

.hx {
  height: 10rpx;
  background-color: #f4f4f4;
}
